h		- height
w		- width

ih	- min-height
xh	- max-height

iw	- min-width
xw	- max-width

pos	- position (a = absolute, f = fixed, r = relative, s = static)

lef	- left
rig	- right
top	- top
bot	- bottom

z		- z-index
ta	- text-align (c = center, l = left, r = right)
wh	- white-space (n = nowrap, p = pre)
ov	- overflow (h = hidden)
tov	- text-overflow (e = ellipsis)


x		- flex
				r = row, c = column
				c = center, s = flex-start, e = flex-end, a = space-around, b = space-between, _ = skip
				b = baseline, c = center, s = flex-start, e = flex-end, t = stretch, _ = skip
				w = wrap, r = wrap-reverse, _ = skip
				Number = flex-grow, _ = skip
				Number = flex-shrink, _ = skip


m 	- margin (eg. 15, '1 2', '10 4 10 13', '10rem 20%')
p 	- padding (eg. 15, '1 2', '10 4 10 13', '10rem 20%')


# Things to be overwritten:
f		- font (eg. 103420)
bg	- background (eg. 'whi', 'blu', 'lime', 'teal', 'pink')
mix	- mixins (eg. 'td', 'box')
